# .env.local
# Environment variables for LOCAL DEVELOPMENT
BASE_DOMAIN=localhost
ENTRYPOINTS=web
ENABLE_TLS=false
TLS_RESOLVER=
TLS_OPTIONS=
MIDDLEWARES=
REDIRECT_MIDDLEWARE=

SECRET_KEY=SecretKeyHere
LOG_LEVEL=DEBUG

ACME_ENABLED=false
ACME_EMAIL=

# No HTTPS port needed locally (but keep it defined to avoid errors)
HTTPS_PORT=443
DYNAMIC_CONFIG=./dynamic-local.yml
# Use a dummy directory for letsencrypt (won't be used)
LETSENCRYPT_DIR=./letsencrypt-disabled
