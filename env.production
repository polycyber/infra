# .env.production
# Environment variables for PRODUCTION DEPLOYMENT
BASE_DOMAIN=your-domain.com
ENTRYPOINTS=websecure
ENABLE_TLS=true
TLS_RESOLVER=myresolver
TLS_OPTIONS=securetls@file
MIDDLEWARES=security-headers@file
REDIRECT_MIDDLEWARE=redirect-to-https@file

SECRET_KEY=SecretKeyHere
LOG_LEVEL=INFO
ACME_ENABLED=true
ACME_EMAIL=infra@polycyber.io
HTTPS_PORT=443
DYNAMIC_CONFIG=./dynamic.yml
LETSENCRYPT_DIR=../letsencrypt
